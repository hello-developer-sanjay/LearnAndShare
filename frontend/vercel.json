{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Expect-CT",
          "value": "max-age=0"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NPM_FLAGS": "--legacy-peer-deps"
    }
  }
}

